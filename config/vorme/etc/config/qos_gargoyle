
config upload 'upload'
	option default_class 'uclass_3'
	option total_bandwidth '550'

config download 'download'
	option default_class 'dclass_3'
	option qos_monenabled 'true'
	option total_bandwidth '3900'

config download_class 'dclass_1'
	option name 'VoIP'
	option percent_bandwidth '1'
	option min_bandwidth '80'
	option max_bandwidth '200'
	option minRTT 'Yes'

config download_class 'dclass_2'
	option name 'Fast'
	option percent_bandwidth '64'

config download_class 'dclass_3'
	option name 'Slow'
	option percent_bandwidth '1'

config download_class 'dclass_4'
	option name 'Normal'
	option percent_bandwidth '14'

config download_class 'dclass_5'
	option name 'game'
	option percent_bandwidth '20'
	option min_bandwidth '1024'
	option minRTT 'Yes'

config download_rule 'download_rule_100'
	option class 'dclass_2'
	option test_order '100'
	option srcport '53'

