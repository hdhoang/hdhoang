
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ar933x_wmac'
	option hwmode '11ng'
	option channel '10'
	option htmode 'HT40-'

config wifi-iface 'ap_g'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option disassoc_low_ack '0'
	option ssid 'autonomy'
	option encryption 'psk2'
	option key 'mot ngay tinh co'

config wifi-iface 'ap_gn_g'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option disassoc_low_ack '0'
	option is_guest_network '1'
	option macaddr '44:55:ff:c0:d3:69'
	option ssid 'nha14'
	option isolate '1'
	option encryption 'psk2'
	option key 'thaohienbop'

