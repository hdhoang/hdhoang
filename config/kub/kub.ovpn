# -*- mode: conf-space -*-
dev tun
topology subnet
persist-key
persist-tun
tls-version-min 1.2
tls-cipher TLS-DHE-RSA-WITH-AES-128-CBC-SHA
ca {{h}}/secrets/wosign.crt
pkcs12 {{h}}/secrets/{{ansible_hostname}}.zahe.me.pfx

tls-server
dh {{h}}/secrets/dh2048
user nobody
group nobody

mode server
keepalive 10 120

server 10.255.0.0 255.255.255.0
push "route {{ address }}"
