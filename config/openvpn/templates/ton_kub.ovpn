# -*- mode: conf-space -*-
dev tun
topology subnet
persist-key
persist-tun
tls-version-min 1.2
tls-cipher TLS-DHE-RSA-WITH-AES-128-CBC-SHA
ca {{h}}/secrets/wosign.crt
pkcs12 {{h}}/secrets/{{ansible_hostname}}.zahe.me.pfx

nobind
tls-client

remote k.zahe.me 22
verify-x509-name k.zahe.me name
remote-cert-tls server
resolv-retry infinite

pull
