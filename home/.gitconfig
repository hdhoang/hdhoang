[user]
	name = Hoàng Đức Hiếu
[color]
	ui = auto
[pack]
	threads = 0
[diff]
	renamelimit = 0
[alias]
	p = !git pull && git log ORIG_HEAD..
	l = log -p --word-diff
	ci = commit -m
	d = diff --word-diff

[url "git://git.kde.org/"]
    insteadOf = kde:
[url "ssh://git@git.kde.org/"]
    pushInsteadOf = kde:
