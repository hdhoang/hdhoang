! -*- mode: conf-xdefaults -*-
! ~/.Xmodmap

keycode 133 = BackSpace

keycode 108 = Escape
remove mod1 = Escape

remove control = Control_L
keycode 37  = Return
clear lock
add control = Caps_Lock
keycode 66  = Control_L

keycode 91  = Delete
keycode 104 = Return
keycode 26  = period greater ellipsis U203D ellipsis U203D

keycode 192 = NoSymbol
keycode 193 = NoSymbol
keycode 194 = NoSymbol
keycode 195 = NoSymbol
keycode 196 = NoSymbol

!dvp
keycode 24  = apostrophe quotedbl
 keycode 52 = semicolon colon

keycode 35  = asciitilde at
 keycode 49 = dollar asciicircum
