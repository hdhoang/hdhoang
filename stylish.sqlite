INSERT INTO "styles" VALUES(3,NULL,'',NULL,'decor',':link[href*="tvtropes.org/"]::before
{
content: url(http://tvtropes.org/favicon.ico);
}
@-moz-document domain("tvtropes.org") {
:link[href*="tvtropes.org/"]::before
{content: none}
:link:not([href*="tvtropes.org/"])::before
{content: url(http://static.mediatropes.info/pmwiki/pub/external_link.gif)}
}

:link[href*=".archive.org/"]::before
{
content: url(http://web.archive.org/favicon.ico);
}
@-moz-document domain(".archive.org") {
:link[href*=".archive.org/"]::before
{content: none}
}

:link[href*="apenwarr.ca/"]::after
{
